package 배열;

public class Sample04 {

	public static void main(String[] args) {
		
		int[] ball = new int[45];
		int i;
		
		for(i=0; i < ball.length; i++)
			ball[i] = i + 1;
		
		
		int tmp;
		int j;
		
		for(i=0; i<6; i++) {
			j = ((int)Math.random() * 45);
			
			tmp = ball[i];
			ball[i] = ball[j];
			ball[j] = tmp;
		}

		for(i=0;i<6; i++) {
			System.out.print(ball[i] + " ");
		}
		
	}

}
